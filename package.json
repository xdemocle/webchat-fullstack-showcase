{
  "name": "docler-webchat-fullstack",
  "version": "0.0.1",
  "description": "Docler Webchat",
  "scripts": {
    "clean": "rimraf ./dist && mkdir dist",
    "start": "run-p dev ts-script-dev",
    "dev": "yarn clean && webpack-dev-server --open",
    "ts-script": "ts-node --project server/tsconfig.json server/index.ts",
    "ts-script-dev": "ts-node-dev --project server/tsconfig.json --inspect -- server/index.ts",
    "prebuild": "yarn clean",
    "build": "cross-env NODE_ENV=production webpack",
    "serv-prod": "cross-env NODE_ENV=production run-p build ts-script",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xdemocle/docler-webchat-fullstack.git"
  },
  "author": {
    "name": "Rocco Russo",
    "email": "xdemocle@gmail.com",
    "url": "https://github.com/xdemocle"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "bitbar-webpack-progress-plugin": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "run-node-webpack-plugin": "^1.2.2",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "3.7.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/styles": "^4.10.0",
    "@types/express": "^4.17.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/redux": "^3.6.0",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.33",
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-translate": "^7.0.1",
    "redux": "^4.0.5",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint"
    ]
  }
}
